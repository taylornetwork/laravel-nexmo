{
    "name": "taylornetwork/laravel-nexmo",
    "require": {
        "illuminate/support": ">=5.5",
        "php": ">=7.3",
        "nexmo/laravel": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Taylor",
            "email": "sam@taylornetwork.ca"
        }
    ],
    "autoload": {
        "psr-4": {
            "TaylorNetwork\\LaravelNexmo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TaylorNetwork\\LaravelNexmo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TaylorNetwork\\LaravelNexmo\\NexmoServiceProvider"
            ],
            "aliases": {
                "NccoBuilder": "TaylorNetwork\\LaravelNexmo\\Facades\\NccoBuilder"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^5.1"
    }
}
